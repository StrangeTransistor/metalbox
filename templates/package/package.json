  "description": "",

  "homepage":            "https://github.com/StrangeTransistor/metalbox",
  "repository": { "url": "https://github.com/StrangeTransistor/metalbox.git", "type": "git" },
  "bugs":       { "url": "https://github.com/StrangeTransistor/metalbox/issues" },

  "engines":
  {
    "node": ">=7"
  },

   "bin": "",
  "main": "",

  "metalbox":
  {
    "presets":
    {
      "dev": [ "Library/Dev", { "options": { "targets": "node-first" }} ],
      "npm": [ "Library",     { "options": { "targets": "node-first" }} ]
    }
  },

  "files":
  [
  ],

  "scripts":
  {
    "start":
      "metalbox preset dev",


    "st":
      "eslint src/ test/",

    "flow":
      "flow check",


    "unit":
      "npm start -- -1 && npm run mocha",

    "mocha":
                      "mocha    --recursive release/dev/test/",
    "cover":
      "istanbul cover _mocha -- --recursive release/dev/test/",


    "test":
      "npm run st && npm run flow && npm run unit",


    "release":
      "metalbox preset npm"
  },

  "peerDependencies":
  {
  },

  "dependencies":
  {
  },

  "devDependencies":
  {
    "metalbox":
      "1",


    "eslint":
      "3",

    "js-outlander":
      "StrangeTransistor/js-outlander#2.1.1",

    "flow-bin":
      "0.53",

    "flow-typed":
      "2",


    "mocha":
      "3",

    "chai":
      "3",

    "istanbul":
      "0.4"
  }
}
