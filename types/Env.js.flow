
import T_Rootpath from 'rootpath';

export type EnvOut =
{
	dst: T_Rootpath
};

export type EnvIn =
{
	src: T_Rootpath
};

export type EnvInOut = EnvIn & EnvOut;

export type EnvRelease = EnvInOut;

export type EnvVersion =
{
	version: string
};

export type EnvFrontend
= EnvRelease
& EnvVersion
&
{
	buckets_path?: string
};

export type EnvBackend = EnvRelease & EnvVersion;

export type EnvNotify =
{
	notifier: T_ReleaseNotify
};


import typeof {default as T_Debug} from '../src/printer';

export type EnvPrinter =
{
	printer: T_Printer
};
