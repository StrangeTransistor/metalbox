{
     "name": "metalbox",
  "version": "1.0.0-pre2",
  "private":  true,

  "metalbox":
  {
    "presets":
    {
      "frontend":
      [
        "Frontend",
        {
          "src": "test/_/release-src/frontend"
        }
      ],
      "frontend-dev":
      [
        "Frontend/Dev",
        {
          "src": "test/_/release-src/frontend"
        }
      ],
      "backend":
      [
        "Backend",
        {
          "src": "test/_/release-src/backend"
        }
      ],
      "backend-dev":
      [
        "Backend/Dev",
        {
          "src": "test/_/release-src/backend"
        }
      ],
      "library":
      [
        "Library",
        {
          "src": "test/_/release-src/library"
        }
      ]
    }
  },

  "bin": "run/metalbox",

  "scripts":
  {
    "flow-typed":
      "flow-typed install",

    "st":
      "eslint src/ test/",

    "flow":
      "flow check",

    "mocha":
                      "mocha    --recursive test/artifact/ test/producer/ test/release/",
    "cover":
      "istanbul cover _mocha -- --recursive test/artifact/ test/producer/ test/release/",

    "test":
      "npm run st && npm run flow && npm run mocha",

    "infer":
      "flow type-at-pos",

    "suggest":
      "flow suggest"
  },

  "dependencies":
  {
    "mkdirp":
      "0.5",

    "fs-sync":
      "1",

    "rootpath":
      "StreetStrider/rootpath#v1.7.0",

    "command-promise":
      "2",

    "find-root":
      "1",

    "commander":
      "2.9",

    "bluebird":
      "3",

    "globule":
      "1",

    "globjoin":
      "0.1",

    "chokidar":
      "1",

    "debounce":
      "1",

    "is-empty":
      "1",

    "indent-string":
      "3",


    "pug":
      "2.0.0-beta6",


    "less":
      "2.7",

    "postcss":
      "5",

    "autoprefixer":
      "6",

    "cssnano":
      "3.4",


    "rollup":
      "0.41",

    "rollup-plugin-includepaths":
      "0.1",

    "rollup-plugin-node-globals":
      "1",

    "rollup-plugin-node-builtins":
      "2",

    "rollup-plugin-node-resolve":
      "2",

    "rollup-plugin-commonjs":
      "7",

    "rollup-plugin-json":
      "2",

    "rollup-plugin-flow":
      "1",

    "babel-core":
      "^6.19",

    "babel-preset-babili":
      "0.0.9",


    "node-notifier":
      "4",

    "cli-color":
      "1"
  },

  "devDependencies":
  {
    "eslint":
      "3",

    "babel-eslint":
      "7",

    "js-outlander":
      "StrangeTransistor/js-outlander#2.1.0",

    "flow-bin":
      "0.37",

    "flow-typed":
      "2",

    "mocha":
      "3",

    "chai":
      "3",

    "istanbul":
      "0.4",

    "dir-compare":
      "1.3",

    "randomstring":
      "1",

    "es6y-throttle":
      "=1.0.1"
  }
}