{
     "name": "metalbox",
  "version": "0.0.0",
  "private":  true,

  "scripts":
  {
    "postinstall":
      "flow-typed install",

    "st":
      "eslint src/ test/",

    "flow":
      "flow check",

    "mocha":
                      "mocha    --recursive test/artifact/ test/producer/ test/release/",
    "cover":
      "istanbul cover _mocha -- --recursive test/artifact/ test/producer/ test/release/",

    "test":
      "npm run st && npm run flow && npm run mocha",

    "infer":
      "flow type-at-pos",

    "suggest":
      "flow suggest"
  },

  "main": "src/",

  "dependencies":
  {
    "mkdirp":
      "0.5",

    "fs-sync":
      "1",

    "rootpath":
      "StreetStrider/rootpath#v1.7.0",

    "bluebird":
      "3",

    "globule":
      "1",

    "chokidar":
      "1",

    "debounce":
      "1",


    "pug":
      "2.0.0-beta6",


    "less":
      "2.7",

    "postcss":
      "5",

    "autoprefixer":
      "6",

    "cssnano":
      "3.4",


    "rollup":
      "0.40",

    "rollup-plugin-includepaths":
      "0.1",

    "rollup-plugin-node-globals":
      "1",

    "rollup-plugin-node-builtins":
      "2",

    "rollup-plugin-node-resolve":
      "2",

    "rollup-plugin-commonjs":
      "7",

    "rollup-plugin-json":
      "2",

    "babel-core":
      "^6.19",

    "babel-preset-babili":
      "0.0.9",


    "node-notifier":
      "4"
  },

  "devDependencies":
  {
    "eslint":
      "3",

    "js-outlander":
      "StrangeTransistor/js-outlander#2.0.10",

    "flow-bin":
      "0.37",

    "flow-typed":
      "2",

    "mocha":
      "3",

    "chai":
      "3",

    "dir-compare":
      "1.3",

    "randomstring":
      "1",

    "es6y-throttle":
      "=1.0.1"
  }
}